{
  "name": "@navikt/react-intl-bundler",
  "version": "0.3.0",
  "description": "Package for building react-intl message-bundles.",
  "repository": "github:navikt/react-intl-bundler",
  "homepage": "https://github.com/navikt/react-intl-bundler",
  "main": "dist/index.js",
  "bin": {
    "react-intl-bundler": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "example": "npm run build && node dist/index.js example/texts-src example/texts-built --typescript"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "NAV",
  "license": "MIT",
  "dependencies": {
    "argparse": "^1.0.10",
    "js-logger": "^1.4.1"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.34",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-intl": "^2.3.8",
    "fs-extra": "^6.0.0",
    "jest": "^23.4.1",
    "react": "^16.4.0",
    "react-intl": "^2.4.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
